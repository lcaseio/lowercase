{
  "name": "HTTP JSON Flow",
  "version": "0.1.0-alpha.7",
  "start": "first",
  "steps": {
    "first": {
      "type": "httpjson",
      "url": "https://swapi.dev/api/planets/1/",
      "headers": {
        "Content-Type": "application/json"
      },
      "on": {
        "success": "second"
      }
    },
    "second": {
      "type": "httpjson",
      "url": "${steps.first.result.json.films[0]}"
    }
  }
}
